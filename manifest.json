{
    "manifest_version": 3,
    "name": "Grünt Radio Discord RPC",
    "version": "1.0",
    "description": "Displays the currently playing song from the web Grünt Radio player on Discord.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.gruntmag.com/radio-player*"],
        "js": ["content.js"]
      }
    ]
  }
  